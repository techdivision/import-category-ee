{
  "shortcuts": {
    "ee": {
      "catalog_category": {
        "validate": [
          "general/general/global-data",
          "general/general/move-files",
          "general/catalog_category/validate"
        ],
        "delete": [
          "general/general/move-files",
          "general/general/global-data",
          "ee/catalog_category/delete"
        ],
        "replace": [
          "general/general/move-files",
          "general/general/cache-warmer",
          "general/general/global-data",
          "ee/catalog_category/sort",
          "ee/catalog_category/delete",
          "ee/catalog_category/replace",
          "ee/catalog_category/replace.path",
          "ee/catalog_category/replace.url-rewrite",
          "general/catalog_category/children-count"
        ],
        "add-update": [
          "general/general/move-files",
          "general/general/cache-warmer",
          "general/general/global-data",
          "ee/catalog_category/sort",
          "ee/catalog_category/add-update",
          "ee/catalog_category/add-update.path",
          "ee/catalog_category/add-update.url-rewrite",
          "general/catalog_category/children-count"
        ]
      }
    }
  },
  "operations": {
    "ee": {
      "catalog_category": [
        {
          "name": "sort",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
	            {
	              "id": "import_category_ee.subject.sortable.bunch",
                  "create-imported-file": false,
	              "file-resolver": {
	                "prefix": "category-import"
	              },
	              "listeners": [
	                {
	                  "subject.export.start": [
	                    "import_category.listener.sort.category"
	                  ]
	                }
	              ],
	              "observers": [
	                {
	                  "import": [
	                    "import_category.observer.category.copy"
	                  ]
	                }
	              ]
	            }
              ]
            }
          ]
        },
        {
          "name": "delete",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_category_ee.subject.bunch",
                  "file-resolver": {
                    "prefix": "category-import"
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category_ee.observer.clear.category"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "replace",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_category_ee.subject.bunch",
                  "file-resolver": {
                    "prefix": "category-create"
                  },
                  "params": {
                    "copy-images": false
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category_ee.observer.composite.create.replace"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "replace.path",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_category_ee.subject.bunch",
                  "file-resolver": {
                    "prefix": "category-path"
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category_ee.observer.category.path"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "replace.url-rewrite",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_category_ee.subject.bunch",
                  "file-resolver": {
                    "prefix": "category-url-rewrite"
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category_ee.observer.url.rewrite"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "add-update",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_category_ee.subject.bunch",
                  "file-resolver": {
                    "prefix": "category-create"
                  },
                  "params": {
                    "copy-images": false
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category_ee.observer.composite.add_update"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "add-update.path",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_category_ee.subject.bunch",
                  "file-resolver": {
                    "prefix": "category-path"
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category_ee.observer.category.path"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "add-update.url-rewrite",
          "plugins": [
            {
              "id": "import.plugin.subject",
              "subjects": [
                {
                  "id": "import_category_ee.subject.bunch",
                  "file-resolver": {
                    "prefix": "category-url-rewrite"
                  },
                  "observers": [
                    {
                      "import": [
                        "import_category_ee.observer.url.rewrite.update"
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  }
}